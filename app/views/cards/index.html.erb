<h1>Cards</h1>
<%= link_to 'New Match', new_match_path(selected_mtg_set: @selected_mtg_set), class: 'btn btn-warning' %>
<br/>
<% @cards.each_with_index do |card, index| %>
  <div class="col-sm-3 spaced">
    <%= link_to card do %>
      <%= image_tag card.image_url, class: 'card-image' %>
    <% end %>
  </div>
<% end %>
